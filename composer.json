{
    "name": "lingoda/ai-sdk",
    "type": "library",
    "description": "Framework-agnostic PHP SDK for AI providers with typed results and platform abstraction",
    "keywords": ["ai", "openai", "anthropic", "gemini", "machine-learning", "chatgpt", "claude"],
    "license": "MIT",
    "authors": [
        {
            "name": "Balazs Csaba",
            "email": "csaba.balazs@lingoda.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "google-gemini-php/client": "^2.5",
        "mozex/anthropic-php": "^1.1",
        "nyholm/psr7": "^1.8",
        "openai-php/client": "^v0.16",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^1.0|^2.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "symfony/http-client": "^6.4|^7.0",
        "symfony/http-client-contracts": "^3.6",
        "symfony/lock": "^6.4|^7.0",
        "symfony/rate-limiter": "^6.4|^7.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.7",
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "symplify/easy-coding-standard": "^12.0",
        "symfony/var-dumper": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lingoda\\AiSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lingoda\\AiSdk\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
